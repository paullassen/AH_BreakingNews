{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Make Donald Drumpf Again",
  "description": "Replaces 'Trump' name with 'Drumpf and replaces some of trump pictures with laughing Obama",
  "version": "2.2",

  "content_scripts":[{
    "js": ["drumpf_script.js"],
    "matches": ["*://*/*"],
    "run_at": "document_end"
    }],
    "browser_action": {
      "default_icon": "drumpf-icon.png"
    }
}
